#include <iostream>
#include <string>

using namespace std;

int main() {
    string str;
    char c;
    
    getline(cin, str);
    cin >> c;
    
    int count = 0;
    for (char ch : str) {
        if (ch == c) {
            count++;
        }
    }
    
    cout << count << endl;
    
    return 0;
}
